      "@angular-monorepo/product-store.mapper": [
        "libs/store-app/product/core/mapper/product-store.mapper/src/index.ts"
      ],
      "@angular-monorepo/product-store.mid": [
        "libs/store-app/product/mid/product-store.mid/src/index.ts"
      ],
      "@angular-monorepo/product-store.model": [
        "libs/store-app/product/model/product-store.model/src/index.ts"
      ],
      "@angular-monorepo/ui": ["libs/shared/ui/src/index.ts"],
      "@angular-monorepo/ui-new-product-store": [
        "libs/store-app/product/ui/ui-new-product-store/src/index.ts"
      ]
